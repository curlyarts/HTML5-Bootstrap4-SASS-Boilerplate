{
  "name": "html5-bootstrap4-scss-boilerplate",
  "version": "0.0.1",
  "description": "The HTML5-Bootstrap4-SCSS Boilerplate to quickstart a HTML5-Bootstrap4-SCSS project!!",
  "main": "",
  "scripts": {
    "compile:sass": "node-sass assets/sass/main.scss assets/css/styles.comp.css",
    "prefix:css": "postcss assets/css/styles.comp.css --use=autoprefixer -o assets/css/styles.prefix.css",
    "compress:css": "node-sass assets/css/styles.prefix.css assets/css/styles.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "watch:sass": "node-sass assets/sass/main.scss assets/css/styles.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass"
  },
  "browserslist": [
    "last 10 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/curlyarts/HTML5-Bootstrap4-SCSS-Boilerplate.git"
  },
  "author": "curlyarts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/curlyarts/HTML5-Bootstrap4-SCSS-Boilerplate/issues"
  },
  "homepage": "https://github.com/curlyarts/HTML5-Bootstrap4-SCSS-Boilerplate#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.10.2",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  }
}
